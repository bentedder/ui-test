- core_navigation = ['Home', 'Destinations', 'Shop', 'Hotels', 'Flights', 'Contact']
- sub_navigation = ['Images', 'Video', 'Games']

%header#header
  %img.logo{src: "images/logo.png"}
  %a.menutab
    menu
  %nav.navigation
    %ul
      - core_navigation.each do |item|
        %li
          %a{href: '#'}
            = item

  %nav.sub-navigation
    %ul
      - sub_navigation.each do |item|
        %li
          %a{href: '#'}
            = item
